# Next.js Frontend Environment Variables
# -------------------------------------
# This file contains environment variables used by the Next.js frontend application.
# Copy this file to .env.local in the same directory and fill in your values.
# NEVER commit the actual .env.local file to version control.

# API URLs
# Required: URLs for backend services
NEXT_PUBLIC_API_URL= 
NEXT_PUBLIC_AI_CHAT_CORE_URL= 

# Authentication
# Required: NextAuth configuration
NEXTAUTH_URL= 
NEXTAUTH_SECRET= 

# Supabase
# Required: Supabase connection details
NEXT_PUBLIC_SUPABASE_URL= 
NEXT_PUBLIC_SUPABASE_ANON_KEY= 

# Google OAuth
# Required: Google OAuth credentials for authentication
GOOGLE_CLIENT_ID= 
GOOGLE_CLIENT_SECRET= 
GOOGLE_REDIRECT_URI= 

# Feature Flags
# Optional: Control feature availability
NEXT_PUBLIC_ENABLE_CHAT_HISTORY=true
NEXT_PUBLIC_ENABLE_PROMPT_REPOSITORY=true
NEXT_PUBLIC_ENABLE_USER_SETTINGS=true

# Analytics
# Optional: Analytics configuration
NEXT_PUBLIC_ANALYTICS_ID= 

# Deployment
# Optional: Deployment-specific configuration
NEXT_PUBLIC_DEPLOYMENT_ENV=development # development, staging, production
NEXT_PUBLIC_VERCEL_URL=